{
  "plugins": [
    "@typescript-eslint",
    "prettier",
  ],
  "extends": [
    "airbnb-typescript-prettier",
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-debugger": "off",
    "no-console": 0
  }
}